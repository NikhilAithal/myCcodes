#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    // Convert user's input to row and column numbers
    int row = (n - 1) / 3;
    int col = (n - 1) % 3;

    // Output the row and column numbers
    printf("%d %d\n", row, col);

    return 0;
}
