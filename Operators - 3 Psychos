#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {

    int x1, y1, x2, y2;
    scanf("%d", &x1);
    scanf("%d", &y1);
    scanf("%d", &x2);
    scanf("%d", &y2);


    float midX = (x1 + x2) / 2.0;
    float midY = (y1 + y2) / 2.0;


    printf("Arun's house is located at(%.1f,%.1f)\n", midX, midY);

    return 0;
}
